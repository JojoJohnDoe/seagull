{
    "name": "@seagull/pipeline",
    "version": "1.2.0",
    "description": "Code pipeline for the Seagull Framework",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "bin": {
        "seagull-pipeline": "./bin/pipeline.js"
    },
    "scripts": {
        "build": "rm -rf dist && tsc",
        "test": "NODE_ENV=test tslint --fix src/**/*.ts test/**/*.ts && nyc mocha --opts test/mocha.opts",
        "test:watch": "npm run test -- --watch --reporter min",
        "docs": "rm -rf docs && typedoc --mode modules --ignoreCompilerErrors --excludeExternals --excludePrivate --excludeNotExported --out docs src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/seagull-js/seagull.git"
    },
    "keywords": [
        "web",
        "framework",
        "web framework",
        "serverless",
        "cloud",
        "aws",
        "react",
        "react.js",
        "isomorphic"
    ],
    "author": "Peter Melinat",
    "license": "LGPL-3.0",
    "bugs": {
        "url": "https://github.com/seagull-js/seagull/issues"
    },
    "homepage": "https://github.com/seagull-js/seagull#readme",
    "devDependencies": {
        "@seagull/testing": "^1.2.0",
        "@seagull/tslint": "^1.2.0",
        "@types/lodash": "^4.14.107",
        "@types/mock-require": "^2.0.0",
        "@types/yaml": "^1.0.1",
        "mock-require": "^3.0.2"
    },
    "dependencies": {
        "@aws-cdk/aws-codebuild": "^0.19.0",
        "@aws-cdk/aws-codecommit": "^0.19.0",
        "@aws-cdk/aws-codepipeline": "^0.19.0",
        "@aws-cdk/aws-iam": "^0.19.0",
        "@aws-cdk/cdk": "^0.19.0",
        "@seagull/commands": "^1.2.0",
        "aws-cdk": "^0.18.1",
        "aws-sdk": "*",
        "chalk": "^2.4.1",
        "dotenv": "^6.1.0",
        "lodash": "^4.17.5",
        "yaml": "^1.0.1"
    },
    "peerDependencies": {
        "typescript": "*"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "**/*.d.ts",
            "coverage",
            "dist",
            "docs",
            "node_modules",
            "test"
        ],
        "reporter": [
            "html",
            "text",
            "text-summary"
        ]
    },
    "gitHead": "2f2098cb8e421957a8c86541462ef341134fccce",
    "publishConfig": {
        "access": "public"
    }
}
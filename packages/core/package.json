{
  "name": "@seagull/core",
  "version": "0.20.0",
  "description": "Featherweight Zero-Config Typesafe Isomorphic Cloud Framework",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "NODE_ENV=test nyc mocha --opts test/mocha.opts",
    "docs": "rm -rf docs && typedoc --mode modules --ignoreCompilerErrors --excludeExternals --excludePrivate --excludeNotExported --out docs src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seagull-js/seagull.git"
  },
  "keywords": [
    "web",
    "framework",
    "web framework",
    "serverless",
    "cloud",
    "aws",
    "react",
    "react.js",
    "isomorphic"
  ],
  "author": "Maximilian Stroh",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/seagull-js/seagull/issues"
  },
  "homepage": "https://github.com/seagull-js/seagull#readme",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/history": "^4.6.2",
    "@types/lodash": "^4.14.107",
    "@types/mocha": "^5.2.1",
    "@types/mock-fs": "^3.6.30",
    "@types/node": "^9.6.21",
    "@types/query-string": "^5.1.0",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/react-helmet": "^5.0.5",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "mocha-typescript": "^1.1.16",
    "mock-fs": "^4.5.0",
    "nyc": "^12.0.2",
    "source-map-support": "^0.5.4",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "typedoc": "^0.11.1",
    "typedoc-plugin-external-module-name": "^1.1.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "csstips": "^0.2.3",
    "history": "^4.7.2",
    "inflection": "^1.12.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "npmlog": "^4.1.2",
    "prettier": "^1.13.5",
    "query-string": "^6.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "typestyle": "^1.7.2",
    "universal-router": "^6.0.0"
  },
  "peerDependencies": {
    "typescript": "^3.0.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage",
      "dist",
      "docs",
      "node_modules"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary"
    ],
    "all": true
  }
}

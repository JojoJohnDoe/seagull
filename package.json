{
    "name": "@seagull/core",
    "version": "0.11.5",
    "description": "Featherweight Zero-Config Typesafe Isomorphic Cloud Framework",
    "bin": {
        "seagull-server": "dist/src/bin/server.js"
    },
    "browser": "dist/src/client.js",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "scripts": {
        "build": "rm -rf dist && tsc",
        "test": "npm run test:unit",
        "test:unit": "NODE_ENV=test mocha --opts test/mocha.opts",
        "test:unit:watch": "npm run test:unit -- --watch --reporter min",
        "docs": "rm -rf docs && typedoc --mode modules --excludeExternals --excludePrivate --excludeNotExported --exclude \"**/src/bin/**\" --out docs src",
        "release": "npm run test && npm run docs && npm publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/seagull-js/seagull.git"
    },
    "keywords": [
        "web",
        "framework",
        "web framework",
        "serverless",
        "cloud",
        "aws",
        "react",
        "react.js",
        "isomorphic"
    ],
    "author": "Maximilian Stroh",
    "license": "LGPL-3.0",
    "bugs": {
        "url": "https://github.com/seagull-js/seagull/issues"
    },
    "homepage": "https://github.com/seagull-js/seagull#readme",
    "devDependencies": {
        "@types/chai": "^4.1.3",
        "@types/create-react-class": "^15.6.1",
        "@types/enzyme": "^3.1.10",
        "@types/enzyme-adapter-react-16": "^1.0.2",
        "@types/history": "^4.6.2",
        "@types/lodash": "^4.14.107",
        "@types/mocha": "^5.2.0",
        "@types/node": "^9.6.6",
        "@types/query-string": "^5.1.0",
        "@types/react": "^16.3.12",
        "@types/react-dom": "^16.0.5",
        "@types/react-helmet": "^5.0.5",
        "aws-sdk": "^2.104.0",
        "browserify": "^16.2.0",
        "chai": "^4.1.2",
        "clear-require": "^2.0.0",
        "common-shakeify": "^0.4.6",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "jsdom": "^11.9.0",
        "jsdom-global": "^3.0.2",
        "local-web-server": "^2.5.1",
        "mocha": "^5.1.1",
        "mocha-typescript": "^1.1.12",
        "source-map-support": "^0.5.4",
        "ts-node": "^6.0.0",
        "tsconfig-paths": "^3.3.1",
        "tslint-config-prettier": "^1.12.0",
        "typedoc": "^0.11.1",
        "typedoc-plugin-external-module-name": "^1.1.1",
        "typedoc-plugin-internal-external": "^1.0.10",
        "typescript": "^2.8.3",
        "uglify-es": "^3.3.9"
    },
    "dependencies": {
        "create-react-class": "^15.6.3",
        "csstips": "^0.2.3",
        "history": "^4.7.2",
        "is-react": "^1.1.1",
        "lodash": "^4.17.5",
        "query-string": "^6.0.0",
        "react": "^16.3.2",
        "react-dom": "^16.3.2",
        "react-helmet": "^5.2.0",
        "typestyle": "^1.7.2",
        "universal-router": "^6.0.0"
    },
    "peerDependencies": {
        "aws-sdk": "^2.104.0",
        "typescript": "^2.8.3"
    }
}

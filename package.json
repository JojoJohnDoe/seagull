{
  "name": "@seagull/core",
  "version": "0.11.5",
  "description": "Featherweight Zero-Config Typesafe Isomorphic Cloud Framework",
  "bin": {
    "seagull-server": "bin/server.js"
  },
  "browser": "dist/src/client.js",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "NODE_ENV=test mocha --opts test/mocha.opts",
    "test:watch": "npm run test -- --watch --reporter min",
    "test:unit": "NODE_ENV=test mocha --opts test/unit/mocha.opts",
    "test:unit:watch": "npm run test:unit -- --watch --reporter min",
    "test:functional": "NODE_ENV=test mocha --opts test/functional/mocha.opts",
    "test:functional:watch": "npm run test:functional -- --watch --reporter min",
    "docs": "rm -rf docs && typedoc --mode modules --ignoreCompilerErrors --excludeExternals --excludePrivate --excludeNotExported --exclude \"**/src/bin/**\" --out docs src",
    "release": "npm run test && npm run docs && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seagull-js/seagull.git"
  },
  "keywords": [
    "web",
    "framework",
    "web framework",
    "serverless",
    "cloud",
    "aws",
    "react",
    "react.js",
    "isomorphic"
  ],
  "author": "Maximilian Stroh",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/seagull-js/seagull/issues"
  },
  "homepage": "https://github.com/seagull-js/seagull#readme",
  "devDependencies": {
    "@types/browserify": "^12.0.33",
    "@types/chai": "^4.1.3",
    "@types/chokidar": "^1.7.5",
    "@types/create-react-class": "^15.6.1",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/history": "^4.6.2",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/lodash": "^4.14.107",
    "@types/mocha": "^5.2.0",
    "@types/mock-fs": "^3.6.30",
    "@types/mock-require": "^2.0.0",
    "@types/node": "^9.6.6",
    "@types/prettier": "^1.13.0",
    "@types/query-string": "^5.1.0",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/react-helmet": "^5.0.5",
    "@types/require-from-string": "^1.2.0",
    "aws-sdk": "^2.104.0",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "clear-require": "^2.0.0",
    "common-shakeify": "^0.4.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jsdom": "^11.9.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.1.1",
    "mocha-typescript": "^1.1.12",
    "mock-fs": "^4.5.0",
    "mock-require": "^3.0.2",
    "source-map-support": "^0.5.4",
    "ts-node": "^6.0.0",
    "tsconfig-paths": "^3.3.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "typedoc": "^0.11.1",
    "typedoc-plugin-external-module-name": "^1.1.1",
    "typescript": "^2.9.0",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "browserify-incremental": "^3.1.1",
    "chokidar": "^2.0.3",
    "copy-node-modules": "^1.0.4",
    "csstips": "^0.2.3",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "npmlog": "^4.1.2",
    "prettier": "^1.13.4",
    "query-string": "^6.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "require-from-string": "^2.0.2",
    "stream-string": "^2.0.0",
    "ts-simple-ast": "^10.1.0",
    "typestyle": "^1.7.2",
    "universal-router": "^6.0.0"
  },
  "peerDependencies": {
    "aws-sdk": "^2.104.0",
    "typescript": "^2.9.0"
  }
}

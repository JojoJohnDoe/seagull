{
    "name": "@seagull/core",
    "version": "0.9.5",
    "description": "Featherweight Zero-Config Typesafe Isomorphic Cloud Framework",
    "main": "dist/lib/index.js",
    "types": "dist/lib/index.d.ts",
    "engines": {
        "node": ">= 6.10.3 < 7"
    },
    "engineStrict": true,
    "scripts": {
        "lint": "tslint -c tslint.json 'src/**/*.ts' --fix",
        "prettier": "prettier --single-quote --no-semi --trailing-comma es5 --write 'src/**/*.ts'",
        "prebuild": "npm run lint && npm run prettier",
        "build": "rm -rf dist && tsc",
        "pretest": "npm run build",
        "test": "NODE_ENV=test mocha --opts mocha.opts",
        "test:watch": "NODE_ENV=test mocha-typescript-watch --opts mocha.opts",
        "docs": "typedoc --mode file --excludeExternals --excludePrivate --out docs src/lib",
        "release": "npm run test && npm run docs && npm publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/seagull-js/seagull.git"
    },
    "keywords": [
        "web",
        "framework",
        "web framework",
        "serverless",
        "cloud",
        "aws",
        "react",
        "react.js",
        "isomorphic"
    ],
    "author": "Maximilian Stroh",
    "license": "LGPL-3.0",
    "bugs": {
        "url": "https://github.com/seagull-js/seagull/issues"
    },
    "homepage": "https://github.com/seagull-js/seagull#readme",
    "devDependencies": {
        "@types/chai": "^4.1.2",
        "@types/mocha": "^2.2.48",
        "@types/node": "^8.0.25",
        "chai": "^4.1.2",
        "mocha": "^5.0.4",
        "mocha-typescript": "^1.1.12",
        "prettier": "^1.11.1",
        "source-map-support": "^0.5.4",
        "ts-node": "^5.0.1",
        "tslint": "^5.9.1",
        "tslint-config-prettier": "^1.10.0",
        "typedoc": "^0.11.1",
        "typescript": "^2.7.2"
    },
    "dependencies": {
        "@seagull/package-config": "^0.3.1",
        "@types/aws-lambda": "0.0.16",
        "@types/history": "^4.6.0",
        "@types/lodash": "^4.14.74",
        "@types/react": "^16.0.40",
        "@types/react-dom": "^16.0.4",
        "@types/react-helmet": "^5.0.5",
        "@types/react-router": "^4.0.22",
        "@types/react-router-config": "^1.0.6",
        "@types/react-router-dom": "^4.2.4",
        "aws-sdk": "^2.104.0",
        "aws-sdk-mock": "^1.7.0",
        "dashify": "^0.2.2",
        "history": "^4.7.2",
        "lodash": "^4.17.4",
        "mobx": "^3.3.1",
        "mobx-react": "^4.3.3",
        "mobx-react-router": "^4.0.1",
        "react": "^16.3.0-alpha.1",
        "react-dom": "^16.3.0-alpha.1",
        "react-helmet": "^5.2.0",
        "react-router": "^4.2.0",
        "react-router-config": "^1.0.0-beta.4",
        "react-router-dom": "^4.2.2",
        "reflect-metadata": "^0.1.10",
        "shortid": "^2.2.8",
        "typescript-memoize": "^1.0.0-alpha.3",
        "typestyle": "^1.7.2"
    }
}
{
    "name": "@seagull-js/seagull",
    "version": "0.5.6",
    "description": "Featherweight Zero-Config Typesafe Isomorphic Cloud Framework",
    "main": "dist/lib/index.js",
    "types": "dist/lib/index.d.ts",
    "engines": {
        "node": ">= 6.10.3 < 7"
    },
    "engineStrict": true,
    "scripts": {
        "lint": "tslint -c tslint.json 'src/**/*.ts' --fix",
        "prettier": "prettier --single-quote --no-semi --trailing-comma es5 --write 'src/**/*.ts'",
        "prebuild": "npm run lint && npm run prettier",
        "build": "rm -rf dist && tsc",
        "pretest": "npm run build",
        "test": "NODE_ENV=test mocha --opts mocha.opts",
        "watch": "NODE_ENV=test mocha-typescript-watch --opts mocha.opts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/seagull-js/seagull.git"
    },
    "keywords": [
        "web",
        "framework",
        "web framework",
        "serverless",
        "cloud",
        "aws",
        "react",
        "react.js",
        "isomorphic"
    ],
    "author": "Maximilian Stroh",
    "license": "LGPL-3.0",
    "bugs": {
        "url": "https://github.com/seagull-js/seagull/issues"
    },
    "homepage": "https://github.com/seagull-js/seagull#readme",
    "devDependencies": {
        "@types/chai": "^4.0.4",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.0.25",
        "chai": "^4.1.1",
        "mocha": "^3.5.0",
        "mocha-typescript": "^1.1.7",
        "prettier": "^1.7.4",
        "source-map-support": "^0.4.16",
        "ts-node": "^3.3.0",
        "tslint": "^5.7.0",
        "tslint-config-prettier": "^1.3.0",
        "typescript": "^2.5.2"
    },
    "dependencies": {
        "@types/aws-lambda": "0.0.16",
        "@types/history": "^4.6.0",
        "@types/lodash": "^4.14.74",
        "@types/react": "^16.0.18",
        "@types/react-dom": "^16.0.2",
        "@types/react-router": "^4.0.15",
        "@types/react-router-config": "^1.0.4",
        "@types/react-router-dom": "^4.0.8",
        "aws-sdk": "^2.104.0",
        "dashify": "^0.2.2",
        "history": "^4.7.2",
        "lodash": "^4.17.4",
        "mobx": "^3.3.1",
        "mobx-react": "^4.3.3",
        "mobx-react-router": "^4.0.1",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-router": "^4.2.0",
        "react-router-config": "^1.0.0-beta.4",
        "react-router-dom": "^4.2.2",
        "reflect-metadata": "^0.1.10",
        "shortid": "^2.2.8",
        "typestyle": "^1.4.0"
    }
}